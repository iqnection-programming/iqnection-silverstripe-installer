# For a complete list of core environment variables see
# https://docs.silverstripe.org/en/4/getting_started/environment_management/#core-environment-variables

#SS_BASE_URL="https://example.com"

# DB credentials
SS_DATABASE_CLASS="MySQLDatabase"
SS_DATABASE_SERVER="localhost"
SS_DATABASE_USERNAME="<db_username>"
SS_DATABASE_PASSWORD="<db_password>"
SS_DATABASE_NAME="<database_name>"

# WARNING: in a live environent, change this to "live" instead of dev
SS_ENVIRONMENT_TYPE="dev"

#SS_DEFAULT_ADMIN_USERNAME="<default_username>"
#SS_DEFAULT_ADMIN_PASSWORD="<default_user_password>"

SS_ERROR_LOG="ss-errors.log"

